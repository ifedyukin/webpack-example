    "style-loader": "0.23.1",
    "terser-webpack-plugin": "1.2.3",
    "url-loader": "1.1.2",
    "webpack": "4.28.4",
    "webpack-assets-manifest": "3.1.1",
    "webpack-cli": "3.2.1",
    "webpack-dev-server": "3.1.14",
    "webpack-notifier": "1.7.0"
  },
  "dependencies": {
    "@babel/polyfill": "7.2.5",
