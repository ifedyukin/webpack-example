{
  "name": "webpack-example",
  "version": "1.0.0",
  "description": "Simple webpack app",
  "scripts": {
    "serve": "cd dist && ws --spa index.html",
    "build": "webpack --mode production --progress -p",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --hot --inline --progress --watch",
    "watch": "nodemon --exec npm run dev",
    "lint": "eslint src lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ifedyukin/webpack-example.git"
  },
  "author": "Igor Fedyukin <me@ifedyukin.ru>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ifedyukin/webpack-example/issues"
  },
  "homepage": "https://github.com/ifedyukin/webpack-example#readme",
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/plugin-proposal-class-properties": "7.3.0",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/preset-env": "7.2.3",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.5",
    "clean-webpack-plugin": "1.0.0",
    "copy-webpack-plugin": "4.6.0",
    "cross-env": "5.2.0",
    "css-loader": "2.1.0",
    "cssnano": "4.1.10",
    "eslint": "5.13.0",
    "file-loader": "3.0.1",
    "html-webpack-plugin": "3.2.0",
    "mini-css-extract-plugin": "0.5.0",
    "nodemon": "1.18.9",
    "postcss-import": "12.0.1",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.5.0",
    "pug": "2.0.3",
